{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "minecraft_to_launch": {
      "type": "object",
      "properties": {
        "version": {
          "type": "string"
        },
        "modification": {
          "enum": ["fabric", "forge", "quilt", null]
        }
      },
      "required": ["version"]
    },
    "run_after_install": {
      "type": "boolean"
    },
    "show_in_vanilla_versions": {
      "type": "array",
      "items": {
        "enum": ["releases", "snapshots", "old_beta", "old_alpha"]
      }
    }
  },
  "required": ["minecraft_to_launch", "run_after_install", "show_in_vanilla_versions"]
}